   "source": [
    "import numpy as np\n",
    "def missing_num(n):\n",
    "    last_num= n[0,-1]\n",
    "    sum_of_seq= last_num*(last_num+1)/2\n",
    "    sum_= sum(n)\n",
    "    return sum_of_seq-sum_\n",
    "    "
   ]
